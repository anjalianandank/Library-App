<body>
  <%- include('./partials/navbar')  %>
  <br><br>

    <div class="container marginTop marginBottomReduce minHeight mb-5" >
        <div class="row justify-content-center">
            <div class="col-md-12 col-8 " style="z-index: 100;">
                
                <main>
                    
                  <section class="tm-content tm-contact">
                    <h2 class="mb-4 tm-content-title">Welcome Back...!</h2>
                    <p class="mb-85">Please enter your login credentials to continue.</p>
                    <form id="contact-form" action="books" onsubmit="return lvalidate()">
                        <div class="form-group mb-4">
                            <input type="email" id="email" class="form-control" placeholder="Email"/>
                        </div>
                        <div class="form-group mb-4">
                            <input type="password" id="password" class="form-control" placeholder="Password"/>
                        </div>
                        <!-- <br>
                        <p class="">Password Strength :</p>
